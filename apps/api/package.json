{
  "name": "rol-api",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
  "build": "npm --prefix ../../services/sim-core install --no-audit --no-fund && npm --prefix ../../services/sim-core run build && tsc -p tsconfig.json",
    "start": "node dist/index.js",
  "pretest": "npm run prisma:generate && npm --prefix ../../services/sim-core install --no-audit --no-fund && npm --prefix ../../services/sim-core run build",
    "test": "vitest run",
  "pretest:ci": "npm --prefix ../../services/sim-core install --no-audit --no-fund && npm --prefix ../../services/sim-core run build",
  "test:ci": "sh -c \"npm run prisma:generate && prisma migrate deploy && vitest run\"",
    "lint": "eslint . --max-warnings=0",
    "etl:azgaar": "tsx src/scripts/etl-azgaar.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:push": "prisma db push",
    "prisma:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/server": "4.11.2",
    "@graphql-tools/schema": "10.0.16",
    "@prisma/client": "^5.20.0",
    "body-parser": "1.20.3",
    "cors": "2.8.5",
    "express": "4.21.1",
    "graphql": "16.9.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^9.13.0",
    "supertest": "^7.0.0",
    "vitest": "^2.1.4",
    "prisma": "^5.20.0",
    "tsx": "4.19.2",
    "typescript": "5.6.3"
  }
}
